<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".MainActivity">
	
	<Button
		android:id="@+id/startChattingSample"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Chatting Sample"
		app:layout_constraintBottom_toTopOf="@id/startMyPageSample"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_chainStyle="packed" />
	
	<Button
		android:id="@+id/startMyPageSample"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="MyPage Sample"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/startChattingSample" />

</androidx.constraintlayout.widget.ConstraintLayout>